{
    "name": "sameyasu/phpcs-added-lines",
    "description": "phpcs wrapper for filtering report based on diff text.",
    "type": "library",
    "require": {
        "sebastian/diff": "^3.0",
        "squizlabs/php_codesniffer": "^3.5",
        "php": "^7.2",
        "monolog/monolog": "^2.1"
    },
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-4": {
            "PhpcsAddedLines\\": [
                "src/",
                "tests/"
            ]
        }
    },
    "config": {
        "platform": {
            "php": "7.2"
        }
    },
    "bin": [
        "bin/phpcs-added-lines"
    ],
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    }
}
